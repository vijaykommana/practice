pipeline{
    agent 'any'
    stages{
        stage ('vcs'){
            steps{
                git url : 'https://github.com/vijaykommana/practice.git',
                branch : 'main'
            }
        }
        stage('build'){
            steps{
                 sh 'docker image build -t spc:1.0 .'
            }
        }
    }
}